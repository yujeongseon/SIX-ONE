<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 매퍼파일의 완전한 경로 : xml생략 -->
<mapper namespace="mybatis.Food">



	<insert id="foodInsert" parameterType="java.util.Map">
		insert into food values(SEQ_FOOD.NEXTVAL,#{foodWhen},#{foodName},#{foodKcal},#{foodDate},#{id})
	</insert>
	
	<select id="selectFoodList" parameterType="java.util.Map" resultType="FoodDTO">
		SELECT * FROM food WHERE id=#{id}
	</select>
	
	
	<delete id="foodreset" parameterType="java.util.Map" >
		DELETE FROM FOOD WHERE id= #{id}
	</delete>
	
	
	
	
	
	
</mapper>