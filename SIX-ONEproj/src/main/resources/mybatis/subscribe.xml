<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 매퍼파일의 완전한 경로 : xml생략 -->
<mapper namespace="mybatis.subscribe">
	
	<select id="subscribeSelectList" parameterType="java.util.Map" resultType="subscribeDTO">
		select subscribe_no, s.routine_no, routine_name 
		from subscribe s join routine r on s.routine_no = r.routine_no  
		where s.id=#{id}
	</select>
	
	<delete id="subscribeDelete" parameterType="java.util.Map">
		delete subscribe where subscribe_no = #{subscribeNo}
	</delete>
	
	<delete id="subscribeDeleteCalendar" parameterType="java.util.Map">
		delete sub_calendar where subscribe_no = #{subscribeNo}
	</delete>
	
	
</mapper>